{
  "name": "express-project-template",
  "version": "1.0.0",
  "description": "Node express project template",
  "main": "srcs/index.js",
  "directories": {
    "lib": "srcs",
    "test": "__tests__"
  },
  "files": [
    "srcs"
  ],
  "scripts": {
    "build": "tsc",
    "build:debug": "tsc --sourceMap",
    "watch": "nodemon --config nodemon.json",
    "test": "mocha -r ts-node/register __tests__/**/*.ts",
    "lint": "tslint -c tslint.json 'srcs/**/*.ts' '__tests__/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Its-Alex/express-project-template.git"
  },
  "keywords": [
    "node",
    "project",
    "template",
    "swagger",
    "express"
  ],
  "author": "Alexandre M",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Its-Alex/express-project-template/issues"
  },
  "homepage": "https://github.com/Its-Alex/express-project-template#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/express-winston": "^3.0.1",
    "@types/joi": "^14.3.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.10.4",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "^3.0.0",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "sinon": "^7.2.7",
    "sinon-mongoose": "^2.2.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.17.3",
    "express-winston": "^3.1.0",
    "joi": "^14.3.1",
    "mongoose": "^5.4.17",
    "swagger-jsdoc": "^3.2.7",
    "swagger-ui-express": "^4.0.2",
    "winston": "^3.2.1",
    "winston-node-sentry": "^1.2.2"
  }
}
